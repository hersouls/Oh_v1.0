# Moonwave Oh Anna Music Player Environment Variables Example

# App Configuration
VITE_APP_NAME="Moonwave Oh Anna Player"
VITE_APP_VERSION="1.0.0"
VITE_APP_DESCRIPTION="평범함에서 특별함으로 - 13곡의 음악 여정"

# API Configuration
VITE_API_BASE_URL="https://api.moonwave.kr"
VITE_APP_URL="https://oh.moonwave.kr"

# Audio Configuration
VITE_AUDIO_BUFFER_SIZE="4096"
VITE_AUDIO_SAMPLE_RATE="44100"

# Feature Flags
VITE_ENABLE_ANALYTICS="true"
VITE_ENABLE_PWA="true"
VITE_ENABLE_OFFLINE="true"

# Development Configuration
VITE_DEV_MODE="development"
VITE_ENABLE_HOT_RELOAD="true"
VITE_ENABLE_SOURCE_MAPS="true"

# Performance Configuration
VITE_ENABLE_COMPRESSION="true"
VITE_ENABLE_CACHING="true"
VITE_ENABLE_PREFETCH="true"